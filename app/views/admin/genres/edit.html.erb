<div class="container">
   <h4>ジャンル編集</h4>
</div>

<%= form_with model:@genre, local: true, url: admin_genre_path(@genre), method: :patch do |f| %>
<div class="container">
  <div class="row">
    <div class="col-lg-2"><p>ジャンル名</p></div>
    <div class="col-lg-2"><%= f.text_field :name %></div>
    <div class="col-lg-2"><%= f.submit '変更を保存' %></div>
  </div>
</div>
<% end %>